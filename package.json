{
  "name": "scroll-snap-slider",
  "version": "3.0.0-RC2",
  "description": "Mostly CSS slider with great performance.",
  "keywords": [
    "slider",
    "carousel",
    "scroll",
    "scroll-snap"
  ],
  "main": "build/ScrollSnapSlider.js",
  "style": "build/scroll-snap-slider.css",
  "module": "build/ScrollSnapSlider.js",
  "types": "build/ScrollSnapSlider.d.ts",
  "files": [
    "build/**/*"
  ],
  "type": "module",
  "sideEffects": false,
  "browserslist": [
    "defaults"
  ],
  "repository": "https://github.com/barthy-koeln/scroll-snap-slider",
  "author": "Barthy Bonhomme <post@barthy.koeln>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "bash-echolorized": "^1.0.1",
    "core-js": "^3.25",
    "eslint": "^8.30.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "sass": "^1.57.1",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "setup-git-hooks": "git config core.hooksPath hooks",
    "build-css": "sass --style expanded src/scroll-snap-slider.scss > build/scroll-snap-slider.css",
    "build-js": "tsc",
    "build": "yarn build-js && yarn build-css",
    "doc": "typedoc src"
  }
}
